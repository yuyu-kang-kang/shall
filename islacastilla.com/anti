<?php
$file = '/path/to/your/protected_file.php';
$backup = '/path/to/backup/protected_file.php';

// Cek apakah file telah dihapus
if (!file_exists($file)) {
    // Jika file dihapus, kembalikan dari backup
    copy($backup, $file);
    echo "File .php telah dihapus, tapi sudah dikembalikan dari backup.";
} else {
    echo "File .php aman.";
}
?>
